<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.samochodgui.NewCarController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Label text="Model" />
            <TextField fx:id="modelPojazduTextField" onAction="#onModelPojazduTextField" HBox.hgrow="ALWAYS" />
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Label text="Nr rejestracyjny" />
            <TextField fx:id="nrRejestracyjnyTextField" onAction="#onNrRejestracyjnyTextField" HBox.hgrow="ALWAYS" />
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Label text="Waga" />
            <TextField fx:id="wagaPojazduTextField" HBox.hgrow="ALWAYS" />
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Label text="Prędkość" />
            <TextField fx:id="predkoscPojazduTextField" HBox.hgrow="ALWAYS" />
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Button fx:id="confirmButton" maxWidth="1.7976931348623157E308" onAction="#onConfirmButton" text="Włącz"
                    HBox.hgrow="ALWAYS"/>
            <Button fx:id="cancelButton" maxWidth="1.7976931348623157E308" onAction="#onCancelButton" text="Wyłącz" HBox.hgrow="ALWAYS" />
        </HBox>
    </VBox>
</AnchorPane>
