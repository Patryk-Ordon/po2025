<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="800.0" prefWidth="1000.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.samochodgui.HelloController">
   <BorderPane prefHeight="542.0" prefWidth="582.0">
      <center>
         <VBox prefHeight="344.0" prefWidth="380.0" BorderPane.alignment="CENTER">
            <children>
              <Button onAction="#onHelloButtonClick" prefHeight="47.0" prefWidth="180.0" text="Hello!" />
            
                    <Label fx:id="welcomeText" />
                <Label>No siema</Label>
                <Label>Co tam?</Label>
              <TextArea>Wpisz tu dużo</TextArea>
                <HBox prefHeight="36.0" prefWidth="426.0">
                    <CheckBox>O ja cię</CheckBox>
                    <CheckBox>O ja cię 2</CheckBox>
                    <CheckBox>O ja cię 3</CheckBox>
                  <ColorPicker />
                </HBox>
            </children>
         </VBox>
      </center>
      <left>
         <Accordion BorderPane.alignment="TOP_LEFT">
           <panes>
             <TitledPane alignment="TOP_CENTER" text="Samochód" textAlignment="CENTER">
               <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0">
                        <children>
                           <VBox layoutX="-1.0" layoutY="-1.0" spacing="5.0">
                              <children>
                                 <HBox alignment="CENTER_LEFT">
                                    <children>
                                       <Label text="Model">
                                          <padding>
                                             <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                          </padding>
                                       </Label>
                                       <TextField HBox.hgrow="ALWAYS" />
                                    </children>
                                    <VBox.margin>
                                       <Insets />
                                    </VBox.margin>
                                 </HBox>
                                 <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="15.0">
                                    <children>
                                       <Label text="Nr rejestracyjny">
                                          <padding>
                                             <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                          </padding>
                                       </Label>
                                       <TextField alignment="CENTER" HBox.hgrow="ALWAYS" />
                                    </children>
                                 </HBox>
                              </children>
                              <padding>
                                 <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                              </padding>
                           </VBox>
                        </children></AnchorPane>
               </content>
             </TitledPane>
             <TitledPane animated="false" text="untitled 2">
               <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
             </TitledPane>
             <TitledPane animated="false" text="untitled 3">
               <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
             </TitledPane>
           </panes>
         </Accordion>
      </left>
      <top>
         <MenuBar BorderPane.alignment="CENTER">
           <menus>
             <Menu mnemonicParsing="false" text="File">
               <items>
                 <MenuItem mnemonicParsing="false" text="Close" />
               </items>
             </Menu>
             <Menu mnemonicParsing="false" text="Edit">
               <items>
                 <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
             </Menu>
             <Menu mnemonicParsing="false" text="Help">
               <items>
                 <MenuItem mnemonicParsing="false" text="About" />
               </items>
             </Menu>
           </menus>
         </MenuBar>
      </top>
   </BorderPane>
</VBox>
